<Window x:Class="Projekt_PBD.DaneKontaktowe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt_PBD"
        mc:Ignorable="d"
        Title="DaneKontaktowe" Height="270" Width="394">
    <Grid>
        <TextBox x:Name="tbxDane" HorizontalAlignment="Center" Margin="0,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="343" Height="143"/>
        <Button x:Name="btnOk" Content="Wyślij" HorizontalAlignment="Left" Margin="26,202,0,0" VerticalAlignment="Top" Width="80" Height="20" Click="btnOk_Click"/>
        <Button x:Name="btnAnuluj" Content="Anuluj" HorizontalAlignment="Left" Margin="289,202,0,0" VerticalAlignment="Top" Width="80" Height="20" Click="btnAnuluj_Click"/>
        <Label Content="Podaj dane kontaktowe i dodatkowe informacje dla właściciela:" HorizontalAlignment="Center" Margin="0,19,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
